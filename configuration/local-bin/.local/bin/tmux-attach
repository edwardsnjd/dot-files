#!/usr/bin/env bash

selected_session=$(
 tmux ls -F "#{session_id}	#S" \
   | fzf --nth=2 \
   | cut -f2
)

currently_in_tmux=$(
  
)

if [ ! -z "${selected_session}" ]
then
  [ ! -z "${TMUX}" ] \
    && tmux switch-client -t "${selected_session}" \
    || tmux attach-session -t "${selected_session}"
fi
